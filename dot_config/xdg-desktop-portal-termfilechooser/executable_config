[filechooser]
cmd=yazi-wrapper.sh
default_dir=$HOME
; Uncomment to skip creating destination save files with instructions in them
; create_help_file=0
; Uncomment and edit the line below to change the terminal emulator command
env=TERMCMD=FZF_DEFAULT_OPTS_FILE="$HOME/.config/fzf/config" kitty --title 'termfilechooser' -o background_opacity=0.75

; Mode must be one of 'suggested', 'default', or 'last'.
open_mode=suggested
save_mode=suggested
